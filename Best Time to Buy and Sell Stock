class Solution {
    public int maxProfit(int[] prices) {
        int minNo = Integer.MAX_VALUE;
        int diff = -1;

        for(int i=0;i<prices.length;i++){
            if(minNo > prices[i]){
                minNo = prices[i];
            }
            diff = Math.max(diff, Math.abs(prices[i] - minNo));
        }

        return diff;
    }
}

