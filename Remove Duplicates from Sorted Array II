class Solution {
    public int removeDuplicates(int[] nums) {
        int n=nums.length;
        if(n==1 || n==2) return n;

        int i=2;
        int k=2;

        while(i < n){
            if(nums[i]!=nums[k-2]){
                nums[k]=nums[i];
                k++;
            }
            i++;
        }

        return k;
    }
}
